@model ProxyBanken.DataAccess.Entity.ProxyTestServer
@{
    Layout = null;
}

<form method="post" action="api/proxytestserverapi/insert">
    @Html.HiddenFor(x => x.Id)
    <div class="form-group">
        <label for="UpdateInterval" class="col-form-label"> @Html.DisplayNameFor(x => x.Name)</label>
        @Html.EditorFor(x => x.Name, new { htmlAttributes = new { @class = "form-control" } })
    </div>

    <div class="form-group">
        <label for="UpdateInterval" class="col-form-label"> @Html.DisplayNameFor(x => x.Url)</label>
        @Html.EditorFor(x => x.Url, new { htmlAttributes = new { @class = "form-control" } })
    </div>
</form>

<script src="~/js/proxy.test.server.insert.js"></script>